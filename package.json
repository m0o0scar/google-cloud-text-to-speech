{
  "name": "@nospoon/text-to-speech",
  "version": "1.0.0",
  "types": "esm/main.d.ts",
  "module": "esm/main.js",
  "main": "build/main.js",
  "files": [
    "build",
    "esm"
  ],
  "scripts": {
    "build:npm": "run-s build:clean build:esm build:cjs",
    "build:clean": "shx rm -rf build esm",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "prepublishOnly": "npm run build:npm",
    "pub:patch": "npm version patch --no-git-tag-version && npm publish",
    "pub:minor": "npm version minor --no-git-tag-version && npm publish",
    "pub:major": "npm version major --no-git-tag-version && npm publish"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^5.0.0",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "find-up": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.4"
  }
}
